<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <object id="window_credit_calc" class="GtkWindow">
        <!--    <property name="default-width">500</property>-->
<!--        <property name="default-height">990</property>-->
        <property name="title">DUCKIN SMART CALC</property>
        <child>
            <object id="grid" class="GtkGrid">
                <property name="row-homogeneous">TRUE</property>
                <property name="column-homogeneous">TRUE</property>
                <property name="column-spacing">5</property>
                <property name="row-spacing">5</property>
                <property name="margin-top">10</property>
                <property name="margin-bottom">10</property>
                <property name="margin-start">10</property>
                <property name="margin-end">10</property>
                <child>
                    <object id="sum_text" class="GtkLabel">
                        <property name="label">Sum</property>
                        <layout>
                            <property name="column">0</property>
                            <property name="column-span">2</property>
                            <property name="row">0</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object id="term_text" class="GtkLabel">
                        <property name="label">Term</property>
                        <layout>
                            <property name="column">0</property>
                            <property name="column-span">2</property>
                            <property name="row">1</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object id="rate_text" class="GtkLabel">
                        <property name="label">Rate</property>
                        <layout>
                            <property name="column">0</property>
                            <property name="column-span">2</property>
                            <property name="row">2</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object id="sum_entry" class="GtkEntry">
                        <property name="can-focus">TRUE</property>
                        <property name="editable">TRUE</property>
                        <property name="xalign">1</property>
                        <layout>
                            <property name="column">2</property>
                            <property name="column-span">4</property>
                            <property name="row">0</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object id="term_entry" class="GtkEntry">
                        <property name="can-focus">TRUE</property>
                        <property name="editable">TRUE</property>
                        <property name="xalign">1</property>
                        <layout>
                            <property name="column">2</property>
                            <property name="column-span">4</property>
                            <property name="row">1</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object id="rate_entry" class="GtkEntry">
                        <property name="can-focus">TRUE</property>
                        <property name="editable">TRUE</property>
                        <property name="xalign">1</property>
                        <layout>
                            <property name="column">2</property>
                            <property name="column-span">4</property>
                            <property name="row">2</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object id="execute_credit_calc" class="GtkButton">
                        <property name="label">Execute</property>
                        <layout>
                            <property name="column">0</property>
                            <property name="row">3</property>
                            <property name="column-span">6</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object id="quit_credit_calc" class="GtkButton">
                        <property name="label">Quit</property>
                        <layout>
                            <property name="column">0</property>
                            <property name="row">4</property>
                            <property name="column-span">6</property>
                        </layout>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>
